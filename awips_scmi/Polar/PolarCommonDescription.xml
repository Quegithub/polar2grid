<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- To be placed in .../edex/data/utility/common_static/site/<site>/satellite/goesr/descriptions/Polar/ -->
<goesrProductDescriptions>
    <default>
        <matches>
            <match pattern="OR-T\d+-\w+-\w+-\w+-\w+_s\d{14}_c\d{14}.nc">
                <attribute name="dataset_name" />
            </match>
        </matches>
    </default>
    <description>
        <source>
            <attribute name="production_location" />
        </source>
        <physicalElement>
            <attribute name="product_name" />
        </physicalElement>
        <creatingEntity>
            <attribute name="satellite_id" />
        </creatingEntity>
        <dataTime>
            <formattedRefTime dateFormat="yyyy-MM-dd'T'HH:mm:ss">
                <attribute name="start_date_time" />
            </formattedRefTime>
        </dataTime>
        <sectorID>
            <attribute name="grid_name" />
        </sectorID>
        <data>
            <variable name="data" />
        </data>
    </description>
</goesrProductDescriptions>
